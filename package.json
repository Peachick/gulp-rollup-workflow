{
  "name": "trandition-no-pack",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clear": "cd build && node clear",
    "sass": "sass ./sass:./style --style=compressed --no-source-map --watch",
    "gulp:dev": "cd build && cross-env  gulp",
    "rollup": "node_modules/.bin/rollup -c build/rollup.config.js -w",
    "build": "concurrently \"yarn run rollup\" \"yarn run clear\" \"yarn run gulp:dev\""
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/jquery": "^3.5.5",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-eval": "^6.22.0",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^17.0.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "7",
    "gulp-connect": "^5.7.0",
    "gulp-file-include": "^2.3.0",
    "gulp-pug": "^4.0.1",
    "gulp-sass": "^4.1.0",
    "open": "^7.3.0",
    "os": "^0.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^4.2.2"
  }
}
